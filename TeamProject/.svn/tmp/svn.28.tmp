@CHARSET "UTF-8";
/* 상단 메뉴 헤더 CSS */
body {
	margin: 0px;
}
header{
	height: 66px;
	width: 100%;
	background: #336699;
	font-size: 12pt;
	font-weight: bold;
	position: relative;
}
.header_nav:before, .header_nav:after{
	content:"";, display: table;
}
.header_nav:after{
	clear:both;
}
.header_nav{
	*zoom:1;
}
header ul{
	display:table;
	padding: 0 0 0 25px;
	height: 40px;
	margin-top:0px;

}
header li{
	display: inline;
	float: left;
	margin-top: 13px;
}
header a{
	color: #fff;
	display: inline-block;
	width: 100px;
	text-align: center;
	text-decoration: none;
	line-height: 40px;
	margin-right: 40px;
}
header a:hover {
	color: #c6d9ec;
}
header li:last-child a{
	border-right: 0;
}
header a#pull{
	display: none;
	padding-top:10px;
	height: 56px;
}
div.loginCheck{
	text-align:right;
	padding-top: 5px;
}
div.loginCheck img{
	width: 20px;
	margin-bottom: -5px;
	margin-right: 5px;
}
div.loginCheck a {
	text-decoration: none;
	color: #777;
	font-size: 11pt;
	font-weight: bold;
}
div.loginCheck a span {
	display: inline-block;
	margin-bottom: 5px;
	
}
div.loginCheck a:hover {
	color: #333399;
}
@media only screen and (max-width : 850px) {
	header {
		border-bottom: 0;
		height: auto;
	}
	header ul {
		display: none;
		height: auto;
	}
	header ul li:nth-child(1){
		margin-top:0px;
	}
	header a {
	  	text-align: left;
	  	width: 100%;
	  	text-indent: 25px;
	  	text-decoration: none;
  	}
	header a#pull {
		display: block;
		width: 100%;
		position: relative;
	}
	header a#pull:after {
		content:"";
		background: url('../image/nav_icon.png') no-repeat;
		width: 30px;
		height: 30px;
		display: inline-block;
		position: absolute;
		right: 15px;
		top: 20px;
	}
	header li {
		display: block;
		float: none;
		width: 100%;
	}
}
